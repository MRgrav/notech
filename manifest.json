{
    "manifest_version": 3,
    "name": "Notech",
    "version": "1.0",
    "description": "A simple note-taking extension.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon120.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon120.png"
    }
  }
  